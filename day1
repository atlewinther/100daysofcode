// instagram portrait 1080 * 1080(1:1 ratio)
size(1080, 1080);

// background(r, g, b)
background(#333333); // I use HEX

// strokeWeight(float)
strokeWeight(2);

// fill(r, g, b)
fill(#F23005);
// ellipse(x, y, px, px)
ellipse(1080/2, 1080/2, 800, 800);

// save image
save("1_100days_of_code.jpg");
